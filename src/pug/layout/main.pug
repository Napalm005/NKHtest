doctype html
html(lang="ru-RU")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport" content="width=device-width,initial-scale=1")
		meta(name="keywords" content="")
		meta(name="description" content="")
		meta(name="format-detection" content="telephone=no")
		link(href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&amp;subset=cyrillic" rel="stylesheet")
		link(rel="stylesheet" type="text/css" href="css/style.min.css")
		title
			block title
	body
		mixin icon(name,className,width,height)
			- mod = mod || ''
			svg(class="" + className + "" width="" + width + "" height="" + height)
				use(xlink:href="img/sprite.svg#icon-" + name)
		mixin scripts(...files)
			each file in files
				script(src= file)

		include ../modules/header
		include ../modules/sidebar
		block content
		+scripts("https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js","js/vendors.min.js","js/scripts.min.js","https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js","https://api-maps.yandex.ru/1.1/index.xml", "//api-maps.yandex.ru/2.1/?lang=ru_RU")

		<!--[if IE]>
			script(src="http://html5shiv.googlecode.com/svn/trunk/html5.js")
		<![endif]-->

